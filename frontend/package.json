{
  "name": "stratification-matrix-viewer",
  "version": "1.0.0",
  "description": "A stratification matrix viewer for neural network data",
  "main": "index.js",
  "author": "Philipp Harth",
  "license": "MIT",
  "repository": {
    "private": true
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@rjsf/core": "^2.5.1",
    "acorn": "^8.4.1",
    "babel-runtime": ">=6.26.0",
    "babylonjs": "^4.1.0",
    "babylonjs-gui": "^4.1.0",
    "babylonjs-loaders": "^6.9.0",
    "babylonjs-materials": "^6.9.0",
    "bcrypt": "^5.1.0",
    "bootstrap": "^4.3.1",
    "cannon": "^0.6.2",
    "codemirror": "^5.56.0",
    "core-js": ">=2.5.7",
    "d3": ">=5.7.0",
    "earcut": "^2.1.5",
    "font-awesome": "^4.7.0",
    "golden-layout": "^1.5.9",
    "html-react-parser": "^1.4.14",
    "imports-loader": ">=0.8.0",
    "indexof": "0.0.1",
    "jquery": "^3.4.1",
    "js-combinatorics": "^1.5.7",
    "jsoneditor": "^9.10.2",
    "jsoneditor-react": "^3.1.2",
    "jszip": "^3.10.1",
    "jszip-utils": ">=0.0.2",
    "meteor-node-stubs": ">=0.2.6",
    "minio": "^7.0.12",
    "oimo": "^1.0.9",
    "paper": "^0.12.11",
    "plotly.js": "^2.24.2",
    "popper.js": ">=1.14.7",
    "prismjs": "^1.29.0",
    "production": "0.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-colorscales": "^0.7.3",
    "react-complex-tree": "^1.1.8",
    "react-d3-graph": "^2.6.0",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.4",
    "react-json-editor-ajrm": "^2.5.14",
    "react-plotly.js": "^2.6.0",
    "react-select": "^5.7.4",
    "react-simple-code-editor": "^0.13.1",
    "react-slider": "^2.0.6",
    "react-vega": "^7.4.2",
    "regl-scatterplot": "^1.6.10",
    "reorder.js": "^1.0.6",
    "science": "^1.9.3",
    "simpl-schema": ">=0.3.0",
    "tree-model": "^1.0.7",
    "vega": "^5.20.2",
    "vega-lite": "^5.1.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.22.5",
    "babel-eslint": ">=7.1.0",
    "eslint": ">=3.10.1",
    "eslint-config-airbnb": ">=13.0.0",
    "eslint-import-resolver-meteor": ">=0.3.3",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jsx-a11y": ">=2.2.3",
    "eslint-plugin-meteor": ">=4.2.2",
    "eslint-plugin-react": ">=6.7.1",
    "prettier": "2.8.8"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "env": {
      "browser": true
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        "error",
        "always"
      ],
      "import/no-extraneous-dependencies": 0,
      "import/extensions": [
        "off",
        "never"
      ],
      "import/no-absolute-path": "off",
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "indent": [
        2,
        4
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "no-mixed-operators": [
        "error",
        {
          "allowSamePrecedence": true
        }
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "func-names": [
        "error",
        "as-needed"
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-console": "off",
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    }
  }
}
